{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "kanbanomaly",
  "version": "1.0.0",
  "identifier": "com.mily.kanbanomaly",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../build"
  },
  "app": {
    "windows": [
      {
        "title": "Kanbanomaly",
        "width": 1080,
        "height": 720
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/favicon.svg",
      "icons/favicon-96x96.png"
    ]
  }
}
